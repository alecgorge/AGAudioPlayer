# Uncomment this line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
inhibit_all_warnings!

target :ios do
    platform :ios, '8.0'
    link_with 'AGAudioPlayer'
    pod 'HysteriaPlayer', :head
end

target :osx do
    platform :osx, '10.10'
    link_with 'AGAudioPlayerOSX'
    pod 'HysteriaPlayer', :head
end